class c {
  var i:int, j:int;
  method c(x:int) {
    i = ++x;
    j = ++x;
  }
  method add (x:int):int {
    i = ++i+x;
    j = ++j-x;
  }
  method print2():int {
    print(i,j);
  }//dynamically typed will fail when returning due to missing return(int)
}

class main {
  var x:int, y:int, o:class c;
  method main() {
    x = 0; y = 0; o = new c(5);
    o.print2();
    o.add(3);
    o.print2();
  }
}

