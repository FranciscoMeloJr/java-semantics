// Testing local variable shadowing a field

class i {
  var f:int;
  method i(v:int) {
    f = v;
  }
  method get():int { return f; }
}

class g {
  var o : class i;
  method g(o : class i) {
    this.o = o;
  }
  method d():int {
    var f:int = 9;
    return (o.get());
  }
}

class main {
  method main() {
    var t : class i = new i(1), y : class g = new g(t);
    print(y.d(), "\n");
  }
}

